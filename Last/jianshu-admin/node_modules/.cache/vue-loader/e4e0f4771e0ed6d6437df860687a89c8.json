              <div>\r\n                {{scope.row.createTime}}\r\n              </div>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <el-pagination\r\n        background\r\n        layout=\"prev,pager,next\"\r\n        :total=\"count\"\r\n        :page-size=\"pageSize\"\r\n        :current-page=\"page\"\r\n        @current-change=\"changePage\"></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      fans: [],\r\n      page: 1,\r\n      pageSize: 0,\r\n      count: 0\r\n    }\r\n  },\r\n  created(){\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    getData(){\r\n      this.$http({\r\n        path: '/fans/findAll',\r\n        method: 'get',\r\n        params: {\r\n          page: this.page,\r\n          author: this.$store.state.user.username\r\n        }\r\n      }).then(res=>{\r\n        this.fans = res.result\r\n        this.page = res.page\r\n        this.pageSize = res.pageSize\r\n        this.count = res.count\r\n      })\r\n    },\r\n    changePage(page){\r\n      this.page = page\r\n      this.getData\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.fans-list{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n</style>"]}]}